{
    "version": 2,
    "functions": {
        "api/assets.php": { "runtime": "vercel-php@0.4.0" },
        "api/index.php": { "runtime": "vercel-php@0.4.0" }
    },
    "routes": [
        {
            "src": "/(css|js)/(.*)",
            "dest": "/api/assets.php?file=public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
